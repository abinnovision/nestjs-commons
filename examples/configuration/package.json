{
	"name": "@internal/example-configuration",
	"version": "0.0.0",
	"private": true,
	"author": {
		"name": "AB INNOVISION GmbH",
		"email": "info@abinnovision.com",
		"url": "https://abinnovision.com"
	},
	"scripts": {
		"build": "nest build",
		"format:check": "prettier --check 'src/**/*.ts' '*.{json{,5},md,y{,a}ml}'",
		"format:fix": "prettier --write 'src/**/*.ts' '*.{json{,5},md,y{,a}ml}'",
		"lint:check": "eslint 'src/**/*.ts'",
		"lint:fix": "eslint 'src/**/*.ts' --fix",
		"start": "nest start",
		"start:debug": "nest start --exec \"node --inspect-brk\" --watch",
		"start:dev": "nest start --watch"
	},
	"lint-staged": {
		"src/**/*.ts": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json{,5},md,y{,a}ml}": "prettier --write"
	},
	"prettier": "@abinnovision/prettier-config",
	"dependencies": {
		"@abinnovision/nestjs-configx": "workspace:^",
		"@nestjs/common": "^11.0.5",
		"@nestjs/core": "^11.0.5",
		"@nestjs/platform-express": "^11.0.5",
		"@nestjs/swagger": "^11.1.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@abinnovision/eslint-config-base": "^3.0.2",
		"@nestjs/cli": "^11.0.2",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.5",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.11.5",
		"@types/express": "^5.0.0",
		"@types/node": "^22.10.10",
		"eslint": "^9.39.1",
		"prettier": "^3.7.4",
		"source-map-support": "^0.5.21",
		"typescript": "^5.8.2",
		"unplugin-swc": "^1.5.9"
	}
}
